    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "husky": "1.1.4",
    "npm-run-all": "4.1.3",
    "prettier": "1.15.2",
    "prettier-check": "2.0.0",
    "pretty-quick": "1.8.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.12.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.16.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.1.6"
  },
